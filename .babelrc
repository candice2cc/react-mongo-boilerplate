{
  "env": {
    "client": {
      "presets": [
        "react",
        [
          "env",
          {
            "targets": {
              "browsers": [
                "> 1%",
                "last 2 versions"
              ]
            }
          }
        ]
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-class-properties",
        "transform-export-extensions",
        "transform-object-rest-spread",
        "transform-es2015-classes"
      ]
    },
    "server": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "10.2.1"
            }
          }
        ]
      ],
      "plugins": [
        "transform-class-properties",
        "transform-export-extensions",
        "transform-object-rest-spread",
        "transform-es2015-classes"
      ]
    }
  }
}
